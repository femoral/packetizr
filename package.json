{
  "name": "packetizr",
  "version": "0.0.2",
  "description": "A code generation and packet inspection tool based on contracts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "packetizr": "./dist/index.js"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "gen": "ts-node src/index.ts gen",
    "test": "jest",
    "check": "prettier --check src/**/*.ts",
    "coverage": "jest --coverage",
    "build": "rimraf dist && tsc && npm run copy-assets",
    "copy-assets": "copyfiles -u 1 ./src/**/*.hbs dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/femoral/packetizr.git"
  },
  "keywords": [
    "packet",
    "serialization"
  ],
  "author": "Felipe Morales",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/femoral/packetizr/issues"
  },
  "homepage": "https://github.com/femoral/packetizr#readme",
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.8",
    "@types/rimraf": "^3.0.0",
    "@types/yargs": "^15.0.8",
    "jest": "^26.5.3",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.1",
    "ts-mockito": "^2.6.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3",
    "copyfiles": "^2.4.0"
  },
  "dependencies": {
    "change-case": "^4.1.1",
    "handlebars": "^4.7.6",
    "yaml": "^1.10.0",
    "yargs": "^16.0.3"
  }
}
